    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
